<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<edmx:Edmx xmlns="http://docs.oasis-open.org/odata/ns/edm"
    xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:DataServices>
        <Schema Namespace="test.pyslet.org">
            <EntityType Name="TypeA">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
                <NavigationProperty Name="AB" Type="test.pyslet.org.TypeB"/> 
            </EntityType>
            <EntityType Name="TypeB">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
            </EntityType>
            <EntityContainer Name="MyDB">
                <EntitySet Name="A" EntityType="test.pyslet.org.TypeA">
                    <!-- If the value of the Target attribute is a
                    SimpleIdentifier, it MUST resolve to an entity set
                    or singleton defined in the same entity container as
                    the enclosing element. -->
                    <NavigationPropertyBinding Path="AB" Target="B"/>
                </EntitySet>
            </EntityContainer>
            <EntityContainer Name="YourDB">
                <EntitySet Name="B" EntityType="test.pyslet.org.TypeB"/>
            </EntityContainer>            
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
