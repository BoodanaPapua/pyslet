<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm"
            Namespace="vocab.pyslet.org" Alias="Vocab">
          <Annotation Term="Vocab.Description">
            <String>A simple vocabulary for testing</String>
          </Annotation>
          <Term Name="Description" Type="Edm.String">
            <Annotation Term="Vocab.Description" String="Which comes first,
                the Annnotation or the Term?" />
          </Term>
          <!-- Annotation elements that are children of an
          edm:Annotations element MUST NOT provide a value for the
          qualifier attribute if the parent edm:Annotations element
          provides a value for the qualifier attribute -->
          <Annotations Target="vocab.pyslet.org.Age" Qualifier="en">            
            <Annotation Term="Vocab.Description" String="Age of item"
                Qualifier="de"/>
          </Annotations>
          <Term Name="Age" Type="Edm.Int32" DefaultValue="10">
          </Term>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
